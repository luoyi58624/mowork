{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^m-(.*)": "./components/my-components/m-$1/m-$1.vue",
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    {
      "path": "tabbar/home/home",
      "style": {
        "navigationBarTitleText": "首页"
      }
    },
    {
      "path": "tabbar/user/user",
      "style": {
        "navigationBarTitleText": "我的"
      }
    },
    {
      "path": "pages/test/map-echarts",
      "style": {
        "navigationBarTitleText": "Echarts地图"
      }
    },
    {
      "path": "pages/test/map-ucharts",
      "style": {
        "navigationBarTitleText": "Ucharts地图"
      }
    },
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "uni-app"
      }
    },
    {
      "path": "pages/login/login",
      "style": {
        "navigationBarTitleText": "用户登录"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "white",
    "navigationBarTitleText": "MoWork",
    "navigationBarBackgroundColor": "#0078d4",
    "backgroundColor": "#fafafa"
  },
  "tabBar": {
    "color": "#222222",
    "selectedColor": "#0078d4",
    "borderStyle": "black",
    "backgroundColor": "#fafafa",
    "list": [
      {
        "pagePath": "tabbar/home/home",
        "iconPath": "static/tabbar/home.png",
        "selectedIconPath": "static/tabbar/home_active.png",
        "text": "首页"
      },
      {
        "pagePath": "tabbar/user/user",
        "iconPath": "static/tabbar/user.png",
        "selectedIconPath": "static/tabbar/user_active.png",
        "text": "我的"
      }
    ]
  }
}
